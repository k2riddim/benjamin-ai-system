# Benjamin AI Agentic App Configuration (example)

# Logging
AGENTIC_APP_LOG_LEVEL=INFO
AGENTIC_APP_LOGS_DIR=/home/chorizo/projects/benjamin_ai_system/agentic_app/logs
AGENTIC_APP_LOG_FILE=agentic_app.log

# Services
AGENTIC_APP_DATA_API_BASE_URL=http://127.0.0.1:8010
AGENTIC_APP_TELEGRAM_INTEGRATION_ENABLED=true
AGENTIC_APP_TELEGRAM_API_BASE_URL=http://127.0.0.1:8011

# OpenAI / LangChain
AGENTIC_APP_OPENAI_API_KEY=YOUR_OPENAI_KEY
AGENTIC_APP_OPENAI_MODEL=gpt-4o-mini
AGENTIC_APP_EMBEDDING_MODEL=text-embedding-3-small
LANGCHAIN_PROJECT=benjamin-agentic
LANGSMITH_API_KEY=

# Qdrant
AGENTIC_APP_QDRANT_URL=http://127.0.0.1:6333
AGENTIC_APP_QDRANT_API_KEY=

# Database
AGENTIC_APP_DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DBNAME

# Timeouts and flags
AGENTIC_APP_HTTP_TIMEOUT=30
AGENTIC_APP_REQUEST_LOGGING=false
AGENTIC_APP_CONTEXT_DEBUG=false


